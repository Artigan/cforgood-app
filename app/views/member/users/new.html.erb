<div class="container-fluid admin">
  <!-- SIDEBAR -->
  <%= render "member/dashboard/shared/sidebar" %>
  <div class="connexion connexion-form-supervisor signup">
    <div class="sign-box signup">
      <div class="sign-user">
        <div class="form-card">
         <div class="subscription-form subscription-form-supervisor">
           <%= simple_form_for(@employee, url: member_users_path) do |f| %>
           <%= f.hidden_field :supervisor_id, value: current_user.id %>
           <%= f.input :email , :class => 'input-form-supervisor'%>
           <%= f.input :first_name , :class => 'input-form-supervisor' %>
           <%= f.input :last_name , :class => 'input-form-supervisor'%>
           <%= f.input :city , :class => 'input-form-supervisor'%>
           <%= f.hidden_field :zipcode , :class => 'input-form-supervisor'%>
           <%= f.submit :class => 'validate-form' %>
           <% end %>
         </div>
       </div>
      </div>
    </div>
  </div>
</div>
